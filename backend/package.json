  "description": "SuCAR - Car Wash Pickup Booking System Backend",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "migrate": "ts-node src/migrations/migrate-simple.ts",
    "migrate:create": "ts-node src/migrations/create-tables-auto.ts",
    "migrate:run": "ts-node src/migrations/execute-migration.ts",
    "migrate:auto": "ts-node src/migrations/auto-migrate.ts",
    "migrate:copy": "ts-node src/migrations/copy-sql.ts",
    "migrate:rest": "ts-node src/migrations/migrate-rest.ts",
    "create-admin": "node scripts/create-admin.js",
    "seed": "node scripts/seed-data.js",
    "fix-approvals": "node scripts/fix-approval-status.js"
